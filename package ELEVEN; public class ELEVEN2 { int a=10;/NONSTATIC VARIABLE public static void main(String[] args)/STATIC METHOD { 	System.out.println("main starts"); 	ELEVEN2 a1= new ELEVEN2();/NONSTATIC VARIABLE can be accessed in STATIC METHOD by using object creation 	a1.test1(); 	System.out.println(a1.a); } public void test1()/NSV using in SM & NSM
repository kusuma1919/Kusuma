package ELEVEN;

public class ELEVEN2 {
int a=10;                // NONSTATIC VARIABLE

public static void main(String[] args)        //STATIC METHOD
{
	System.out.println("main starts");
	ELEVEN2 a1= new ELEVEN2();    // NONSTATIC VARIABLE can be accessed in STATIC METHOD by using object creation
	a1.test1();
	System.out.println(a1.a);
}
public void test1()               // NONSTATIC METHOD   
{
	System.out.println(a);
}
}


output: main starts
        10
        10
